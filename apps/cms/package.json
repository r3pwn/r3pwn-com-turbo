{
  "name": "cms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "dotenvx run -f .env -- cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && dotenvx run -f .env -- cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.22.1",
    "@payloadcms/db-mongodb": "^3.12.0",
    "@payloadcms/next": "^3.12.0",
    "@payloadcms/plugin-nested-docs": "^3.12.0",
    "@payloadcms/plugin-seo": "^3.12.0",
    "@payloadcms/richtext-lexical": "^3.12.0",
    "@payloadcms/storage-gcs": "^3.12.0",
    "@repo/payload-common": "workspace:*",
    "cross-env": "^7.0.3",
    "graphql": "^16.10.0",
    "next": "15.1.3",
    "payload": "^3.12.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "0.33.5",
    "lexical": "0.20.0",
    "@lexical/headless": "0.20.0",
    "@lexical/html": "0.20.0",
    "@lexical/link": "0.20.0",
    "@lexical/list": "0.20.0",
    "@lexical/mark": "0.20.0",
    "@lexical/react": "0.20.0",
    "@lexical/rich-text": "0.20.0",
    "@lexical/selection": "0.20.0",
    "@lexical/table": "0.20.0",
    "@lexical/utils": "0.20.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.4",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}